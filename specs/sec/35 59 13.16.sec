         <ALN HORIZONTAL="CENTER" VERTICAL="BOTTOM"/>
      </STY>
      <STY SID="s52">
         <ALN HORIZONTAL="CENTER"/>
      </STY>
   </STS>
   <TDA COLUMNCOUNT="3" ROWCOUNT="3">
      <COL STYLEID="s50" AUTOWIDTH="0" WIDTH="198.00"/>
      <COL STYLEID="s50" AUTOWIDTH="0" WIDTH="160.50"/>
      <COL AUTOWIDTH="0" WIDTH="202.50"/>
      <ROW AUTOHEIGHT="0" HEIGHT="23.25">
         <CEL MERGEDOWN="1" STYLEID="s51">
<DTA TYPE="STRING">SIZE</DTA>
         </CEL>
         <CEL STYLEID="s51">
<DTA TYPE="STRING">ENERGY ABSORPTION</DTA>
         </CEL>
         <CEL STYLEID="s52">
<DTA TYPE="STRING">REACTION FORCE</DTA>
         </CEL>
      </ROW>
      <ROW AUTOHEIGHT="0" HEIGHT="36.00">
